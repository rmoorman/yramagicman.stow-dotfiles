#!/usr/bin/python
import sys
from random import seed, randint, shuffle
seed()
dict = open('/usr/share/dict/cracklib-small')
words = dict.readlines()
words = map(lambda x: x.strip(), words)
words = [word for word in words]
if len(sys.argv) != 2:
    print('need number of words')
    exit()
ws = []
one = randint(0, 100)
two = randint(0, 100)
symbols = ['!', '@', '#', '$', '%', '^', '*', '&', ')',
	   '(', '-', '_', '=', '+', '[', '{', '}', ']', ';', ':', "'", '"',
	   ',', '<', '>', '.', '/', '?', ')', ]

for i in range(0, int(sys.argv[1])):
    ws.append(words[randint(0, len(words))])

for i in range(0, randint(0, int(sys.argv[1]))):
    ws.append(symbols[randint(0, len(symbols) - 1)])

for i in range(0, randint(0, int(sys.argv[1]))):
    ws.append(str(randint(min(one, two), max(one, two))))

passwd = shuffle(ws)

print(''.join(ws))

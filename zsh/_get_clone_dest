function _get_clone_dest() {

    if [[ $1 =~ '^(git@|https)' ]]; then
        tillcolon=("${(@s/:/)1}")
        if [[ $tillcolon[1] = 'https' ]]; then
            atslashes=("${(@s#/#)tillcolon}")
            location="$atslashes[-2]/$atslashes[-1]"
        else
            location="$tillcolon[-1]"
        fi
    else
        location=$1
    fi
}

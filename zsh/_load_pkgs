function _load_pkgs() {
    tput civis
    for name alt in ${(kv)pkgs}
    do
        _source_or_install $name $alt
    done
    local dead=();
    if [[ $#procs > 0 ]]; then
        errs=0
        for p name in ${(kv)procs}; do
            wait $p
            if [[ $? -eq 0 ]]; then
                builtin printf "\n\rCompleted %s " $name
            else
                errs=$(( $errs + 1 ))
                builtin printf "\r\nErrored out on  %s  of  %s %s check %s for more\r\n " $errs $(( $#procs / 2 )) $name "$MODULES_DIR/logs/$p"
                    tput cnorm
                    echo "Remove directory associated with failed clone?"
                    atslashes=("${(@s#/#)name}")
                    command rm -ri "$MODULES_DIR/$atslashes[1]"
                    tput civis
            fi

        done
        echo
    fi
    tput cnorm
}
